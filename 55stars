package fp;

/**
 * Created by RoZvEr on 7/1/2017.
 */
public class FirstProgram {

    public static void main(String[] args) {

        for(int i=1; i<=10; i++) {
            for(int s=1; s<=i; s++) {
                System.out.print("*");
            }
            System.out.println();
        }

    }

}
